{% extends 'BarraBackBundle::layout.html.twig' %}

{% block title %} vPit Measurements {% endblock %}

{% block section %}
    <section>
        <h1>Measurements</h1>
        <ul>
        {% for measurement in measurements %}
            <li>ID: {{ measurement.id }} Type: {{ measurement.type }} In gr:{{ measurement.gr }}</li>
        {% endfor %}
        </ul>

        {{ form(form) }}
    </section>
{% endblock %}