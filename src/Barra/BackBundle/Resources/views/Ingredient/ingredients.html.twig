{% extends 'BarraBackBundle::layout.html.twig' %}

{% block title %} vPit Ingredients {% endblock %}

{% block section %}
    <section>
        <h1>Ingredients</h1>
        <ul>
        {% for ingredient in ingredients %}
            <li> ID: {{ ingredient.id }} Name: {{ ingredient.name }}</li>
        {% endfor %}
        </ul>

        {{ form(form) }}
    </section>
{% endblock %}