{
  "exclude": [
    "app-compiled",
    "app/bootstrap-compiled.ts",
    "node_modules"
  ],
  "compilerOptions": {
    "experimentalDecorators": true,
    "lib": [
      "es6",
      "dom"
    ],
    "moduleResolution": "node",
    "noEmit": true,
    "noImplicitAny": true,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "target": "es5"
  },
  "angularCompilerOptions": {
    "genDir": "app-compiled",
    "debug": false,
    "skipMetadataEmit": true,
    "trace": true
  }
}
