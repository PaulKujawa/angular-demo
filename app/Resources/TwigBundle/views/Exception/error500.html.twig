{% extends '::base.html.twig' %}
{% trans_default_domain "messages" %}
{% block title %}500{% endblock %}

{% block languageBtn %}{% endblock %}

{% block section %}
    <section class="container errorPage">
        <h1>{{ 'app.message.errorCode'|trans({'%code%':status_code}) }}</h1>
        <p>{{ 'app.message.e500'|trans({'%text%':status_text}) }}</p>
        <a href="{{ path('barra_front_me') }}">{{ 'app.word.startpage'|trans() }}</a>
    </section>
{% endblock %}