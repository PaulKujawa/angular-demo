# Cache and logs (Symfony2)
/app/cache/*
/app/logs/*
/app/var/jwt/*
!app/var/jwt/public-test.pem
!app/var/jwt/private-test.pem
!app/cache/.gitkeep
!app/logs/.gitkeep

# Cache and logs (Symfony3)
/var/cache/*
/var/logs/*
!var/cache/.gitkeep
!var/logs/.gitkeep

# Parameters
/app/config/parameters.yml
/app/config/parameters.ini

# Managed by Composer
/app/bootstrap.php.cache
/var/bootstrap.php.cache
/bin/*
!bin/console
!bin/symfony_requirements
/vendor/

# Assets and user uploads
/web/bundles/
/web/css/
/web/images/
/web/js/
/web/vendor/

# Build data
/build/ccHTML/*
/build/logs/*
/build/pdepend/*
!build/ccHTML/.gitkeep
!build/logs/.gitkeep
!build/pdepend/.gitkeep


